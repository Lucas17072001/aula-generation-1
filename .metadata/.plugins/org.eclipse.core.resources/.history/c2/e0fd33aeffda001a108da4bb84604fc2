package exercicios;

//ex7
import java.util.Scanner;
public class ex7 {

	public static void main(String[] args) {
	
		Scanner ler = new Scanner(System.in);
		double A,B,C,D,E,F,X,Y;
		
		System.out.println("Entre com os valores de A até F");
		A = ler.nextDouble();
		B = ler.nextDouble();
		C = ler.nextDouble();
		D = ler.nextDouble();
		E = ler.nextDouble();
		F = ler.nextDouble();
		
		X = ((C*E)-(B*F))/((A*E)-(B*D));
		Y = ((A*F)-(C*D))/((A*E)-(B*D));
		
		System.out.println("Valor de X: "+X+"\nValor de Y:"+Y);
		
	}

}
