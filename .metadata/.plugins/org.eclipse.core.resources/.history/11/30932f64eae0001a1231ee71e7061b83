package teste;

import java.util.Scanner;

public class cinema {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		String vetor[] = new String[10];
		String num_sele;
		int x;

		vetor[0] = "a1";
		vetor[1] = "a2";
		vetor[2] = "a3";

		System.out.println("Qual assento deseja comprar?");
		num_sele = ler.nextLine();
		
		for(x=0;x<9;x++)
		{
			if (num_sele.equals(vetor[x])) 
			{
			System.out.println("Já comprado!");
			}
		}
	}
}
