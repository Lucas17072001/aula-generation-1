package loops;

import java.util.Scanner;

public class ex3 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		int idade,x;
		
		System.out.println("Entre com a idade ou digite '-99' para sair:");	
		idade = ler.nextInt();
		
		while(idade!=-99)
		{
			System.out.println("Entre com a idade: ");	
			idade = ler.nextInt();
		}
	}

}
