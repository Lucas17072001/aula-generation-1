package teste;

import java.util.Scanner;

public class cinema {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		String vetor[] = new String[10];
		String num_sele;
		int x;

		System.out.println("Qual assento deseja comprar?");
		num_sele = ler.nextLine();

		for (x = 0; x < 10; x++) {
			if (num_sele == vetor[x]) {
				System.out.println("O numero selecionado ja foi comprado");
			}
			else
			{
				System.out.println("Assento comprado!");
				vetor[x]=num_sele;
			}
			
			//while (num_sele != vetor[x]) {
				//assento = num_sele;
			//}

		}
	}

}
