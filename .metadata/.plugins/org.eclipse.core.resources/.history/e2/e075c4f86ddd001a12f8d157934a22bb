package exercicios_revisao;

import java.util.Scanner;

public class ex6 {

	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		int vetor[]=new int[3]; 
		int matriz[][]=new int[3][3];
		int lin,col;
		
		//preenche vetor
		for (int i = 0; i < vetor.length; i++)
		{
			System.out.println("Digite um número para o vetor:");
			vetor[i] = ler.nextInt();
		}
		
		//mostra vetor
		for (int i = 0; i < vetor.length; i++)
		{
			System.out.printf("%d \t",vetor[i]);
		}
		
		//preenche matriz
		for(lin=0;lin<3;lin++)
		{
			for(col=0;col<3;col++)
			{
				System.out.println("Digite um número para a matriz");
				matriz[lin][col]=ler.nextInt();
			}
		}
		
		//mostra matriz
		for(lin=0;lin<3;lin++)
		{
			for(col=0;col<3;col++)
			{
				System.out.printf("%d \t");
			}
			System.out.println();
		}
	}

}
